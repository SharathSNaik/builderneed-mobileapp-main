        border-top: 16px solid #3498db;
        /* Blue */
        border-radius: 50%;
        width: 120px;
        height: 120px;
        animation: spin 2s linear infinite;
    }

    @keyframes spin {
        0% {
            transform: rotate(0deg);
        }

        100% {
            transform: rotate(360deg);
        }
    }

    :root {
        --main-color: #111;
        --loader-color: #fff;
        --back-color: #A5D6A7;
        --time: 3s;
        --size: 3px;
    }

    .loader-line {
        width: 100%;
        height: 100%;
        top: 10;
        padding: 10px 10px 10px 10px;
        left: 0;
        position: absolute;
        align-content: center;
        justify-content: flex-start;
        z-index: 100000;
    }

    .loader__element {
        height: var(--size);
        width: 100%;
        background: var(--back-color);

    }

    .loader__element:before {
        content: '';
        display: block;
        background-color: var(--loader-color);
        height: var(--size);
        width: 0;
        animation: getWidth var(--time) ease-in infinite;
    }

    @keyframes getWidth {
        100% {
            width: 100%;
        }
    }
</style>
